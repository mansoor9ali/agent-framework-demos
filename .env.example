# OpenAI Configuration
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL_ID=gpt-4o
OPENAI_API_KEY=your_openai_api_key_here

# MCP (Model Context Protocol) Configuration
# Set to 'true' to enable MCP tools for pizza ordering
ENABLE_MCP=true
MCP_URL=https://your-mcp-server-url.azurecontainerapps.io/mcp
MCP_API_TOKEN=your_mcp_token_here

# Alternative LLM Providers
FOUNDARYLOCAL_API_KEY=OPENAI_API_KEY
FOUNDARYLOCAL_BASE_URL=http://localhost:8000/v1
FOUNDARYLOCAL_MODEL_ID=qwen2.5-14b

SYNTHETIC_API_KEY=your_synthetic_api_key_here
SYNTHETIC_BASE_URL=https://api.synthetic.new/openai/v1
SYNTHETIC_MODEL_ID=hf:openai/gpt-oss-120b

DOTTED_API_KEY=your_dotted_api_key_here
DOTTED_BASE_URL=https://dotted.ngrok-free.app
DOTTED_MODEL_ID=gpt-120b

DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL_ID=deepseek-chat

# Azure Speech Services
AZURE_STT_SUBSCRIPTIONKEY=your_azure_stt_subscription_key_here
AZURE_STT_SUBSCRIPTIONKEY2=your_azure_stt_subscription_key2_here
AZURE_STT_ENDPOINT=https://eastasia.api.cognitive.microsoft.com/
AZURE_STT_REGION=eastasia

AZURE_TTS_SUBSCRIPTIONKEY=your_azure_tts_subscription_key_here
AZURE_TTS_SUBSCRIPTIONKEY2=your_azure_tts_subscription_key2_here
AZURE_TTS_ENDPOINT=https://eastasia.api.cognitive.microsoft.com/
AZURE_TTS_REGION=eastasia

# LLM Configuration
LLM_MODEL=hf:openai/gpt-oss-120b
LLM_BASE_URL=https://api.synthetic.new/openai/v1
LLM_API_KEY=your_llm_api_key_here

# Embedder Configuration
EMBEDDER_MODEL=text-embedding-3-small
EMBEDDER_BASE_URL=https://api.openai.com/v1
EMBEDDER_API_KEY=your_embedder_api_key_here

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DATABASE=agenticMermoryDB
POSTGRES_USER=your_postgres_username
POSTGRES_PASSWORD=your_postgres_password

# Qdrant Configuration
QDRANT_URL=http://localhost:6333

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# Additional API Keys
SERP=your_serp_api_key_here
GITHUB_TOKEN=your_github_personal_access_token_here

# OpenTelemetry Configuration (for Aspire Dashboard)
# Uncomment to enable tracing, metrics, and logs export
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

